{
  "root": true,
  "extends": "eslint-config-airbnb-base",
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "2017"
  },
  "rules": {
    "max-len": 0,
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-multiple-empty-lines": 0,
    "space-before-function-paren": 0,
    "func-names": 0,
    "vars-on-top": 0,
    "no-param-reassign": 0,
    "object-shorthand": 0,
    "import/order": 0,
    "prefer-arrow-callback": 0,
    "global-require": 0,
    "no-multi-spaces": 0,
    "prefer-destructuring": 0,
    "import/newline-after-import": 0,
    "padded-blocks": 0,
    "consistent-return": 0,
    "no-prototype-builtins": 0,
    "import/no-dynamic-require": 0,
    "no-unused-vars": 0
  }
}
